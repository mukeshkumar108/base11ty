{
  "name": "base11ty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "start": "run-p eleventy:dev sass:dev",
    "build": "run-s eleventy:build sass:build",
    "eleventy:dev": "eleventy --serve",
    "eleventy:build": "eleventy",
    "clean": "rimraf dist",
    "sass": "sass src/scss",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist/css",
    "sass:build": "sass --no-source-map --style=compressed src/scss:dist/css && postcss dist/css/*.css --no-map -u autoprefixer -u cssnano -d dist/css",
    "lint:html": "pa11y-ci -c .pa11yci.json './dist/**/*.html'"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.17",
    "bulma": "^0.9.4",
    "cssnano": "^6.0.3",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "sass": "^1.69.7"
  }
}
